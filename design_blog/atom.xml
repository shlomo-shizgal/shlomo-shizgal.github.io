---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Shlomo's design blog</title>
	<link href="http://shlomo-shizgal.github.io/design_blog/atom.xml" rel="self"/>
	<link href="http://shlomo-shizgal.github.io/design_blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://shlomo-shizgal.github.io/design_blog</id>
	<author>
		<name>Shlomo Shizgal</name>
		<email>shlomo.shizgal@zoho.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://shlomo-shizgal.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://shlomo-shizgal.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>

